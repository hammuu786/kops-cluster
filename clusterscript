kops create cluster \
     --name=aahammad.xyz \
     --state=s3://iloveindia \
     --dns-zone aahammad.xyz \
     --node-count 2 \
     --node-size t3.medium \
     --node-volume-size 10 \
     --master-size t3.medium \
     --master-volume-size 10 \
     ##--zones ap-south-1a,ap-south-1b,ap-south-1c --yes
     --zones us-east-1a,us-east-1b,us-east-1c --yes
